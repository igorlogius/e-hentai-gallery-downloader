{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/e-hentai-gallery-downloader",
  "background": {
    "scripts": ["filesaver.js", "jszip.js", "background.js"]
  },
  "browser_action": {
    "default_area": "navbar",
    "default_title": "Download Gallery"
  },
  "manifest_version": 2,
  "name": "e-hentai Gallery Downloader",
  "version": "1.0.4",
  "description": "Download e-hentai.org gallery as zip archives",
  "icons": {
    "16": "icon.png"
  },
  "permissions": [
    "menus",
    "storage",
    "activeTab",
    "*://*.hath.network/*",
    "*://e-hentai.org/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "update_url": "https://github.com/igorlogius/e-hentai-gallery-downloader/raw/main/updates.json"
    }
  }
}
